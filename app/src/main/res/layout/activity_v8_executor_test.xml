<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingStart="20dp"
        android:paddingEnd="80dp"
        tools:context="com.paladin.engine.v8.V8ExecutorTestActivity">

        <Button
            android:id="@+id/evaluateVoidScript"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="evaluateVoidScript" />

        <Button
            android:id="@+id/evaluateStringScript"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="evaluateStringScript" />

        <Button
            android:id="@+id/callJavaScriptFunc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="callJavaScriptFunc" />

        <Button
            android:id="@+id/injectKVObject"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="injectKVObject" />

        <Button
            android:id="@+id/paladinMethodTest"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAllCaps="false"
            android:text="paladinMethodTest" />

    </LinearLayout>

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:rotation="45"
        android:layout_marginTop="18dp"
        android:padding="5dp"
        android:background="@color/paladin_main_color"
        android:textColor="@color/white"
        android:text="V88888" />

</RelativeLayout>